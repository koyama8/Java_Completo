package projeto;

public class Plano {

	public String gympass = "Gympass";;
	public String fiel= "Fiel";
	
	
	
	public void validaPlano(String senha) {
		
		
		if(senha.contains(gympass)) {
			System.out.println("Seu plano é Gympass");
		}else if(senha.contains(fiel)) {
			System.out.println("Seu plano é Fiel");
		}else {
			System.out.println("Voce não selecionou nenhum plano");
		}
		
	}
	
	
	
}
