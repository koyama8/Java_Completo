package estrutura_controle;

import java.util.Scanner;

/**
 * @author Matheus
 *
 */

public class Exercicio04 {


public static void main(String[] args) {
	
   Scanner sc = new Scanner(System.in);
  
  
   System.out.println("Digite um numero");
   int num = sc.nextInt();
   
   for(int i = num; i>0; i--) {
	   
	   System.out.println(i);
   }
   
 }
}
