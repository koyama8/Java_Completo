package orientacao_objetos;

import java.util.Scanner;

public class Exe01 {
  public static void main(String[] args) {
   
	   Scanner sc = new Scanner(System.in);
	   
	
	   String nome = "Joao";
       String cpf = "12345";
       double salario = 1250;
	   int senha = 123;
	   int numeroFuncionarios = 12345;
	   
	   Gerente gerente = new Gerente(nome,cpf,salario,senha,numeroFuncionarios);
	   gerente.setSenha(12345);
	   
       System.out.println(gerente);
       
	   
	   System.out.println();
	   
	   gerente.validaSenha(senha);
	   
	   
	   
       
 }

@Override
public String toString() {
	return "Exe01 [toString()=" + super.toString() + "]";
}
   
  
}
