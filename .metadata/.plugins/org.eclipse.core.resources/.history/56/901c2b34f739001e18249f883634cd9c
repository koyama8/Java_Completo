ackage orientacao_objetos;

public class ordenar {

	public void ordena(comparavel[] objeto) {
		
		for(int i=0; i<objeto.length;i++) {
			for(int j=0; j<objeto.length;j++) {
				
				if(objeto[i].comparar(objeto[j])>0) {
					
					comparavel temp = objeto[i];
					objeto[i] = objeto[j];
					objeto[j] = temp;
				}
			}
		}
	}
}
